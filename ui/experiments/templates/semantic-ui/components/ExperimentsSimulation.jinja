{# def d #}

<div class="ui top attached tabular menu">
    <a class="item active" data-tab="tab-file">File Identification</a>
    <a class="item" data-tab="tab-main">Main Information</a>
    <a class="item" data-tab="tab-detail">Detailed Information</a>
    <a class="item" data-tab="tab-other">Other</a>
</div>
<div class="ui bottom attached segment">
    <div class="ui tab active" data-tab="tab-file">
        <ExperimentsFileIdentification d={d.file_identification} />
    </div>
    <div class="ui tab" data-tab="tab-main">
        <ExperimentsMainInformation d={d.main_information} />
    </div>
    <div class="ui tab" data-tab="tab-detail">
        <ExperimentsDetailedInformation d={d.detailed_information} />
    </div>
    <div class="ui tab" data-tab="tab-other">
        <table class='ui compact definition table'>
            <ITableField d={d._metadata_date} />
            <ITableField d={d._dump_sw_version} />
            <ITableField d={d._gromacs_version} />
            <ITableField d={d._metadump_version} />
            <ITableField d={d._tpx_version} />
            <ITableField d={d._uniprot_id} />
        </table>
    </div>
</div>
