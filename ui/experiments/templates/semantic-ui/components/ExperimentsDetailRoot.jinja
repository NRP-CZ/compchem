{# def md #}

<table class='ui compact definition table'>
  <ITableField d={md.description} />
  <ITableField d={md.name} />
  <ITableField d={md.publisher} />
  {% for item in array(md.creators) %}
    <ITableSection d={item} >
      <ExperimentsCreator d={item} />
    </ITableSection>
  {% endfor %}
  <ITableSection d={md.fundingReference} >
    <ExperimentsFundingReference d={md.fundingReference} />
  </ITableSection>
  {% for item in array(md.objectIdentifiers) %}
    <ITableSection d={item} >
      <ExperimentsNrObjectPid d={item} />
    </ITableSection>
  {% endfor %}
  {% for item in array(md.simulations) %}
    <ITableSection d={item} >
      <ExperimentsSimulation d={item} />
    </ITableSection>
  {% endfor %}
</table>